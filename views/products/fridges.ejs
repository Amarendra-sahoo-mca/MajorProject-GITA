<%- layout("/layouts/boilerplate") %> 
<body>
    <div class="fridgeCont">
        <%for(data of datas){%>
            <div class="fridgeCard">
                <div class="images">
                    <img src=<%=data.image1%> alt="image1">
                    <img src=<%=data.image2%> alt="image2">
                    <img src=<%=data.image3%> alt="image3">
                </div>
                <h2><%=data.name%></h2>
                <p>
                    <b>&#8377; <%=data.price.toLocaleString("en-IN")%></b> <br><br>
                    <%=data.info%> <br>
                    <%=data.description%> 
                </p>
            </div>
        <%}%>
    </div>
</body>