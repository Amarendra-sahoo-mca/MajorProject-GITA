<% layout("/layouts/boilerplate") %> 
<body>
    <%for(data of datas){%>
        <div class="cartCard">
            <div class="cartimg">
                <img src=<%=data.image%> alt="image">
            </div> 
            
            <div class="text">
                <h2><%=data.name%></h2>
                <p> <br>
               <span class="price"> <b>&#8377; <%=data.price.toLocaleString("en-IN")%></b></span>
            </p></div>
        </div>
    <%}%>
</body>